<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="auth">
    <select id="allAuth" parameterType="Map" resultType="vo.AuthorityVO">
        select *
        from authority
        where a_cate = #{a_cate}
    </select>
    

</mapper>